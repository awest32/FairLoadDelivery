! IEEE 13 Node Test Feeder - THREE PHASE THREE WIRE VERSION
! All lines and loads converted to balanced three-phase
! Based on Tennessee Tech University script

Set DefaultBaseFrequency=60

New circuit.IEEE13_3Wire
~ basekv=115 pu=1.0001 phases=3 bus1=SourceBus
~ Angle=30
~ MVAsc3=20000 MVASC1=21000
~ baseMVA=1

!SUB TRANSFORMER DEFINITION
New Transformer.Sub Phases=3 Windings=2 XHL=(8 1000 /)
~ wdg=1 bus=SourceBus conn=Wye kv=115 kva=5000 %r=(.5 1000 /)
~ wdg=2 bus=650 conn=wye kv=4.16 kva=5000 %r=(.5 1000 /)

! FEEDER 3-PHASE VOLTAGE REGULATOR (converted from single-phase bank)
New Transformer.Reg1 phases=3 XHL=0.01 kVAs=[5000 5000]
~ Buses=[650 RG60] kVs=[4.16 4.16] %LoadLoss=0.01
~ wdg=1 conn=wye
~ wdg=2 conn=wye
~ %rs=[0 0]

!TRANSFORMER DEFINITION - remains 3-phase
New Transformer.XFM1 Phases=3 Windings=2 XHL=2
~ wdg=1 bus=633 conn=Wye kv=4.16 kva=500 %r=.55 XHT=1
~ wdg=2 bus=634 conn=Wye kv=0.480 kva=500 %r=.55 XLT=1

!LINE CODES - ALL THREE PHASE
